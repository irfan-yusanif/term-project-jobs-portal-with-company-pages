<script>
    
    function fun(value) {
       // var url = '~/User/search?s=' + value + '&from=json';
        $.post("/User/search1", { s:value },
        function (results) {
            resultss(results);
            
            
        });

    }
    function resultss(results) {
        
        var abc = '<div class="col-md-4"><div class="single-profile"><div class="media"><div class="pull-left"><img class="media-object" src="/Files/u';
        var def = '.jpg" width="100" height="100"/></div><div class="media-body"><h4><a href="../User/Index?id=';
        var ghi = '</a></h4><h5>';
        var jkl = '</h5></div></div></div></div>';
        $("#results").empty();
        $.each(results, function (index) {
            $("#results").append(abc + results[index].Id + def + results[index].Id + '">' + results[index].name + ghi + results[index].about + jkl);
        });
        
    }
</script>
<div class="content-wrapper">
    <div class="container">
        <section style="margin:40px auto">
            <div class="row">
